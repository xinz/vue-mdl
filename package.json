{
  "name": "vue-mdl",
  "version": "0.8.4",
  "description": "Reusable Vue components using Material Design Lite",
  "main": "dist/vue-mdl.js",
  "scripts": {
    "test": "npm run lint && npm run test:unit",
    "start": "NODE_ENV=dev node build/dev-server.js",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "test:unit": "karma start test/unit/karma.conf.js --single-run",
    "build": "npm run build:dev && npm run build:prod",
    "build:prod": "webpack --bail --progress --hide-modules --config build/webpack.dist.prod.conf.js",
    "build:dev": "webpack --bail --progress --hide-modules --config build/webpack.dist.dev.conf.js",
    "dev": "karma start test/unit/karma.dev.conf.js",
    "dev:integration": "node build/dev-server.js",
    "dev:doc": "NODE_ENV=doc node build/dev-server.js",
    "doc": "webpack -p --progress --bail --config build/webpack.gh-pages.conf.js"
  },
  "keywords": [
    "vue",
    "mdl",
    "ui",
    "desgin",
    "google",
    "material",
    "lite",
    "bootstrap",
    "vuestrap",
    "vuemdl",
    "front",
    "normalize",
    "css"
  ],
  "author": "posva",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^1.0.25"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "chai": "^3.5.0",
    "chromedriver": "2.21.2",
    "connect-history-api-fallback": "^1.2.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "function-bind": "^1.1.0",
    "highlight.js": "^9.4.0",
    "html-webpack-plugin": "^2.21.0",
    "http-proxy-middleware": "^0.16.0",
    "inject-loader": "^2.0.1",
    "isparta-loader": "^2.0.0",
    "jade": "^1.11.0",
    "karma": "^0.13.22",
    "karma-chai-dom": "^1.1.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.13.1",
    "lolex": "^1.5.0",
    "material-design-lite": "^1.1.3",
    "mocha": "^2.5.3",
    "nightwatch": "^0.9.4",
    "node-sass": "^3.8.0",
    "ora": "^0.2.3",
    "phantomjs-prebuilt": "^2.1.7",
    "sass-loader": "^3.2.0",
    "selenium-server": "^2.53.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "url-loader": "^0.5.7",
    "vue": "^1.0.25",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.5.2",
    "vue-router": "^0.7.13",
    "vue-style-loader": "^1.0.0",
    "vue-transfer-dom": "^1.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/vue-mdl.git"
  },
  "bugs": {
    "url": "https://github.com/posva/vue-mdl/issues"
  },
  "homepage": "https://github.com/posva/vue-mdl#readme"
}
